{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://fiboa.example/geojson/schema.json",
  "title": "fiboa GeoJSON Encoding Schema",
  "type": "object",
  "required": [
    "id",
    "geometry",
    "bbox",
    "properties"
  ],
  "properties": {
    "id": {
      "$ref": "https://fiboa.example/core/schema.json#/$defs/id"
    },
    "geometry": {
      "$ref": "https://fiboa.example/core/schema.json#/$defs/geometry"
    },
    "bbox": {
      "$ref": "https://fiboa.example/core/schema.json#/$defs/bbox"
    },
    "properties": {
      "type": "object",
      "properties": {
        "datetime": {
          "$ref": "https://fiboa.example/core/schema.json#/$defs/datetime"
        },
        "expires": {
          "$ref": "https://fiboa.example/core/schema.json#/$defs/expires"
        },
        "area": {
          "$ref": "https://fiboa.example/core/schema.json#/$defs/area"
        }
      }
    }
  },
  "allOf": [
    {
      "$ref": "https://geojson.org/schema/Feature.json"
    }
  ]
}